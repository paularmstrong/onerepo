<div class="not-content">
	<div class="graph">
		<div class="content">
			<div id="mermaid" class="mermaid hidden"></div>
			<div class="no-content">
				<p>Graph not found. Run the following to</p>
				<pre>one graph show --open</pre>
				<button
					id="example"
					data-graph-data=""
					>Show an example</button
				>
			</div>
		</div>
	</div>
</div>

<script src="./visualizer.ts"></script>

<style>
	.controls {
		border-bottom: 1px solid var(--sl-color-hairline);
	}
	.graph {
		margin: 1em -100%;
		margin: 1em calc(50% - 50vw);
		height: 90vh;
	}

	.content {
		height: 100%;
		width: 100vw;
		display: flex;
		align-items: center;
		justify-content: center;
	}

	.content .mermaid {
		flex-grow: 0;
		flex-shrink: 0;
		width: 100%;
		height: 100%;
	}

	.content .mermaid.hidden {
		display: none;
	}

	.no-content {
		position: absolute;
	}
</style>

<style is:global></style>
